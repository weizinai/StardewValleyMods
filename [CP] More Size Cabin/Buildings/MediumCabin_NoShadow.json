{
    "Changes": [
        {
            "Action": "Load",
            "Target": "Mods/{{ModId}}/MediumFloatingCabin_Fish, Mods/{{ModId}}/MediumFloatingCabin_Flower, Mods/{{ModId}}/MediumTrailerCabin, Mods/{{ModId}}/MediumWinterGarden",
            "FromFile": "assets/MediumCabin/{{TargetWithoutPath}}.png"
        },
        {
            "Action": "EditData",
            "Target": "Data/Buildings",
            "Entries": {
                "{{ModId}}_MediumCabin_NoShadow": {
                    // Required fields
                    "Name": "{{i18n:Buildings.MediumCabin.Name}}",
                    "Description": "{{i18n:Buildings.MediumCabin.Description}}",
                    "Texture": "Mods/{{ModId}}/MediumTrailerCabin",
                    // Construction
                    "Builder": "Robin",
                    "BuildCondition": "CAN_BUILD_CABIN",
                    // Exterior behavior
                    "Size": {
                        "X": 3,
                        "Y": 2
                    },
                    "HumanDoor": {
                        "X": 1,
                        "Y": 1
                    },
                    // Exterior appearance
                    "DrawShadow": false,
                    "Skins": [
                        {
                            "Id": "MediumFloatingCabin_Fish",
                            "Name": "{{i18n:Buildings.MediumFloatingCabin.Name}}",
                            "Description": "{{i18n:Buildings.MediumFloatingCabin.Description}}",
                            "Texture": "Mods/{{ModId}}/MediumFloatingCabin_Fish"
                        },
                        {
                            "Id": "MediumFloatingCabin_Flower",
                            "Name": "{{i18n:Buildings.MediumFloatingCabin.Name}}",
                            "Description": "{{i18n:Buildings.MediumFloatingCabin.Description}}",
                            "Texture": "Mods/{{ModId}}/MediumFloatingCabin_Flower"
                        },
                        {
                            "Id": "MediumWinterGarden",
                            "Name": "{{i18n:Buildings.MediumWinterGarden.Name}}",
                            "Description": "{{i18n:Buildings.MediumWinterGarden.Description}}",
                            "Texture": "Mods/{{ModId}}/MediumWinterGarden"
                        }
                    ],
                    // Interior
                    "IndoorMap": "FarmHouse",
                    "IndoorMapType": "StardewValley.Locations.Cabin",
                    // Tile interactions
                    "ActionTiles": [
                        {
                            "Id": "Default_OpenMailbox",
                            "Tile": {
                                "X": 1,
                                "Y": 2
                            },
                            "Action": "Mailbox"
                        }
                    ]
                }
            }
        }
    ]
}

